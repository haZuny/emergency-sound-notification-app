<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <!--시크바 배경-->
    <item android:id="@android:id/background">
        <shape android:shape="line">D
            <!-- 시크바 선 굵기 : 길이 10dp -->
            <stroke android:width="6dp" android:color="#D8D8D8" />
            <!-- 시크바 내용 색상 -->
            <solid android:color="#D8D8D8"/>
        </shape>
    </item>

    <!-- 시크바 진행 프로그레스 -->
    <item android:id="@android:id/progress" >
        <clip>
            <shape android:shape="line">
                <!-- 시크바 선 굵기 : 길이 10dp -->
                <stroke android:width="6dp" android:color="#DADDFC" />
                <!-- 시크바 내용 색상 -->
                <solid android:color="#DADDFC"/>
            </shape>
        </clip>
    </item>
</layer-list>